<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="mainframe">
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
      <property name="sticky">nsew</property>
      <rows>
        <row id="0">
          <property name="minsize">28</property>
          <property name="pad">0</property>
          <property name="weight">1</property>
        </row>
        <row id="1">
          <property name="minsize">0</property>
          <property name="pad">0</property>
          <property name="weight">6</property>
        </row>
        <row id="2">
          <property name="minsize">28</property>
          <property name="weight">1</property>
        </row>
        <row id="3">
          <property name="minsize">28</property>
          <property name="weight">1</property>
        </row>
      </rows>
      <columns>
        <column id="0">
          <property name="pad">0</property>
          <property name="weight">3</property>
        </column>
      </columns>
    </layout>
    <child>
      <object class="ttk.Labelframe" id="FilterFrame">
        <property name="takefocus">false</property>
        <property name="text" translatable="yes">Filter:</property>
        <layout>
          <property name="column">0</property>
          <property name="padx">4</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">ew</property>
        </layout>
        <child>
          <object class="ttk.Label" id="Label_2">
            <property name="text" translatable="yes">Background:</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Combobox" id="cmbBackground">
            <property name="height">2</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="Label_3">
            <property name="text" translatable="yes">Species:</property>
            <layout>
              <property name="column">2</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Combobox" id="cmbSpecies">
            <layout>
              <property name="column">3</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="TvFrame">
        <property name="relief">groove</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">nsew</property>
          <rows>
            <row id="0">
              <property name="weight">1</property>
            </row>
            <row id="1">
              <property name="weight">1</property>
            </row>
          </rows>
          <columns>
            <column id="0">
              <property name="weight">2</property>
            </column>
          </columns>
        </layout>
        <child>
          <object class="ttk.Treeview" id="tv">
            <property name="selectmode">extended</property>
            <property name="yscrollcommand">tvScrollbar.set</property>
            <layout>
              <property name="column">0</property>
              <property name="padx">4</property>
              <property name="pady">4</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">nsew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Scrollbar" id="tvScrollbar">
            <property name="orient">vertical</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">ns</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="CommandFrame">
        <layout>
          <property name="column">0</property>
          <property name="padx">4</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property name="sticky">ew</property>
          <rows>
            <row id="0">
              <property name="pad">4</property>
            </row>
          </rows>
          <columns>
            <column id="0">
              <property name="pad">4</property>
            </column>
          </columns>
        </layout>
        <child>
          <object class="ttk.Button" id="cmdDownload">
            <property name="text" translatable="yes">Download Morgue</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="cmdCSV">
            <property name="text" translatable="yes">Export to CSV</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="StatusFrame">
        <property name="padding">4</property>
        <property name="relief">sunken</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="sticky">ew</property>
          <columns>
            <column id="0">
              <property name="weight">1</property>
            </column>
            <column id="1">
              <property name="weight">1</property>
            </column>
          </columns>
        </layout>
        <child>
          <object class="ttk.Label" id="lblStatus">
            <property name="width">20</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Progressbar" id="progressbar">
            <property name="orient">horizontal</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">nsew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Sizegrip" id="Sizegrip_1">
            <layout>
              <property name="column">3</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
